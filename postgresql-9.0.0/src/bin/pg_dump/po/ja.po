#: pg_dump.c:5455
#, c-format
msgid "expected %d check constraint on table \"%s\" but found %d\n"
msgid_plural "expected %d check constraints on table \"%s\" but found %d\n"
msgstr[0] ""
"テーブル\"%2$s\"の検査制約は%1$dと期待していましましたが、%3$dありました\n"
msgstr[1] ""
"テーブル\"%2$s\"の検査制約は%1$dと期待していましましたが、%3$dありました\n"

